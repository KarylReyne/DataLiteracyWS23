	file = {Cone et al. - 2022 - Pandemic Acceleration Covid-19 and the emergency .pdf:/home/tomf/Zotero/storage/M9RIRQV5/Cone et al. - 2022 - Pandemic Acceleration Covid-19 and the emergency .pdf:application/pdf},
